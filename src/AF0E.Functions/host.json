{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "%TaskHubName%"
    }
  },
  "logging": {
    "logLevel": {
      "DurableTask.AzureStorage": "Warning",
      "DurableTask.Core": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "telemetryMode": "OpenTelemetry"
}
